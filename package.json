{
  "name": "pemilu-client",
  "version": "1.0.0",
  "description": "pemilu client",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "build --dir",
    "dist": "build"
  },
  "author": "ahmadfahmip",
  "license": "ISC",
  "build": {
    "appId": "pemilu-client",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "squirrel",
      "icon": "build/icon.ico"
    }
  },
  "devDependencies": {
    "electron": "^3.0.9",
    "electron-builder": "^20.36.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "ejs": "^2.6.1",
    "electron-reload": "^1.3.0",
    "express": "^4.16.4"
  }
}
